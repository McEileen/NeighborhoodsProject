<html>
  <head></head>
  
  <body>
    <h2>Placeholder For Group Rep's Events</h2>
    
    <table>
      <tr>
        <th>Event</th>
        <th>Location</th>
        <th>Time</th>
      </tr>
      <% @events.each do |event| %>
      <tr>
        <td><%= link_to event.name, group_rep_event_path(params[:group_rep_id], event.id) %></td>
        <td><%= event.location %></td>
        <td><%= event.event_time %></td>
      </tr>
      <% end %>
    </table>
    
  </body>
</html>